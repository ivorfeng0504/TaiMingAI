<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <sqlmapping key="InternalRegisterUser">
    <description>
      时间:2017-11-28
      作者:sayook
      功能:内部添加用户
      参数:用户基本信息
    </description>
    <sql>
      <![CDATA[
          INSERT INTO [TmingUserInfo]([Name],[Password],[Mobile],[Email])
          VALUES(@Name,@Password,@Mobile,@Email)
      ]]>
    </sql>
  </sqlmapping>
  <sqlmapping key="GetUserInfoList">
    <description>
      时间:2017-11-28
      作者:sayook
      功能:查询用户列表
      参数:无
    </description>
    <sql>
      <![CDATA[
         select * from TmingUserInfo
      ]]>
    </sql>
  </sqlmapping>
  <sqlmapping key="GetUserInfoById">
    <description>
      时间:2017-12-06
      作者:sayook
      功能:根据ID查询用户信息
      参数:无
    </description>
    <sql>
      <![CDATA[
         select * from TmingUserInfo where id=@id
      ]]>
    </sql>
  </sqlmapping>
</configuration>
